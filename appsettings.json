{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    },
    "File": {
		  "Path": "Logs/app-log-{Date}.txt",
      "Append": true,
      "MinLevel": "Warning",  
      "FileSizeLimitBytes": 0,
      "MaxRollingFiles": 0  
	  }
  },
  "AllowedHosts": "*"
}
